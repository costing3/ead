<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
	
  <display-name>Guestbook Servlets Web Application</display-name>
  <servlet>
  	<display-name>GuestBook Servlet</display-name>
  	<servlet-name>GuestBookServlet</servlet-name>
  	<servlet-class>ro.ucv.inf.ead.guestbook.GuestBookServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GuestBookServlet</servlet-name>
  	<url-pattern>/GuestBook</url-pattern>
  </servlet-mapping>
</web-app>
